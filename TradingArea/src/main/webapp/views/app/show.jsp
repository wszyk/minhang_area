<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%	String path = request.getContextPath();			String basePath = request.getScheme() + "://"					+ request.getServerName() + ":" + request.getServerPort()					+ path + "/";%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html><head><title>商圈平台管理系统</title><script type="text/javascript">		var _basePath = '<%=basePath%>';</script><meta http-equiv="pragma" content="no-cache"><meta http-equiv="cache-control" content="no-cache"><meta http-equiv="expires" content="0"><meta http-equiv="keywords" content="keyword1,keyword2,keyword3"><meta http-equiv="description" content="This is my page"><jsp:include page="../adminCommon.jsp"></jsp:include><c:if test="${loginUser.role == 2}"><style type="text/css">.layui-header {    z-index: 1000;    height: 0px!important;}.layui-layout-admin .layui-side {    top: 0px!important;    width: 200px;    overflow-x: hidden;}.layui-layout-admin .layui-body {    top: 0px!important;    bottom: 44px;}</style></c:if></head><body class="show-body">	<div class="layui-layout layui-layout-admin">		<jsp:include page="appHeader.jsp"></jsp:include>		<jsp:include page="appLeft.jsp"></jsp:include>		<div class="layui-tab layui-tab-brief">			<div id="layuiTabContent" class="layui-body layui-tab-content app-content">				<div class="layui-tab-item layui-show">					<div class="layui-main">						<div id="LAY_preview">							<div class="layui-tab layui-tab-brief" lay-filter="analysis">								<ul class="layui-tab-title app-tab-title">									<li data-id="1" class="layui-this">总量分析</li>									<li data-id="2">趋势分析</li>									<li data-id="3">对比分析</li>								</ul>								<div class="layui-tab-content">									<div class="layui-tab-item layui-show">										<form class="layui-form" action="" lay-filter="form1">											<div class="layui-form-item">												<div>													<input type="radio" name="dateTime1" value="month1" title="月度" checked>													<input type="radio" name="dateTime1" value="quarter1" title="季度" >													<input type="radio" name="dateTime1" value="year1" title="年度" >													&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;													<input type="radio" name="comContent1" data-unit="万元"  value="sales1" title="销售额" checked>													<input type="radio" name="comContent1" data-unit="个"  value="mids1" title="主要商户数" >													<input type="radio" name="comContent1" data-unit="笔"  value="salesNums1" title="交易笔数">												</div>											</div>											<div class="app-statistics app-statistics-left">											<div id="totalAnalysis1"></div>											</div>											<div class="app-statistics app-statistics-right">											<div id="totalAnalysis2"></div>											</div>										</form>									</div>									<div class="layui-tab-item">										<form class="layui-form" action="" lay-filter="form2">											<div class="layui-form-item">												<div>													<input type="radio" name="dateTime2" value="month2" title="月度" checked>													<input type="radio" name="dateTime2" value="quarter2" title="季度" >													<input type="radio" name="dateTime2" value="year2" title="年度">													&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;													<input type="radio" name="comContent2" data-unit="万元"  value="sales2" title="销售额" checked>													<input type="radio" name="comContent2" data-unit="个"  value="mids2" title="主要商户数" >													<input type="radio" name="comContent2" data-unit="笔"  value="salesNums2" title="交易笔数">													<select class="app-select" lay-ignore id="comContent2" name="comContent2" lay-filter="industry">														<option value="">全部行业</option>														<c:forEach items="${industryList }" var="industry">															<option value="${industry}">${industry }</option>														</c:forEach>													</select>												</div>											</div>											<div style="visibility:hidden;" class="app-statistics">											<div id="trendAnalysis"></div>											</div>																					</form>									</div>									<div class="layui-tab-item">										<form class="layui-form" action="" lay-filter="form3">											<div class="layui-form-item">												<div>													<span class="app-select-time">时间：</span>													<select class="app-select time" lay-ignore id="comTime31" name="comTime31" lay-filter="startTime">														<c:forEach var="x" begin="${nowYear-1 }" end="${nowYear }">															<option value="${x }">${x }年</option>														</c:forEach>													</select>													<select class="app-select time" lay-ignore id="comTime311" name="comTime311" lay-filter="startTime">														<c:forEach var="x" begin="1" end="12">															<c:if test="${x < 10 }">															<option value="0${x }">${x }月</option>															</c:if>															<c:if test="${x >= 10 }">															<option value="${x }">${x }月</option>															</c:if>														</c:forEach>													</select>													<span class="app-select-time">~&nbsp;&nbsp;</span>													<select class="app-select time" lay-ignore id="comTime32" name="comTime32" lay-filter="endTime">														<c:forEach var="x" begin="${nowYear-1 }" end="${nowYear }">															<c:if test="${endYear == x }">															<option selected value="${x }">${x }年</option>															</c:if>															<c:if test="${endYear != x }">															<option value="${x }">${x }年</option>															</c:if>														</c:forEach>													</select>													<select class="app-select time" lay-ignore id="comTime322" name="comTime322" lay-filter="endTime">														<c:forEach var="x" begin="1" end="12">															<c:if test="${x < 10 }">															<option value="0${x }">${x }月</option>															</c:if>															<c:if test="${x >= 10 }">																<c:if test="${endMonth == x }">																<option selected value="${x }">${x }月</option>																</c:if>																<c:if test="${endMonth != x }">																<option value="${x }">${x }月</option>																</c:if>															</c:if>														</c:forEach>													</select>													<span style="margin-left: 20px;" class="app-select-time">行业：</span>													<select class="app-select" lay-ignore id="comContent3" name="comContent3" lay-filter="industry">														<option value="">全部行业</option>														<c:forEach items="${industryList }" var="industry">															<option value="${industry}">${industry }</option>														</c:forEach>													</select>													<!-- <input type="radio" name="dateTime3" value="month3" title="月度" checked>													<input type="radio" name="dateTime3" value="quarter3" title="季度" >													<input type="radio" name="dateTime3" value="year3" title="年度" > -->													&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;													<input type="radio" name="comContent3" data-unit="万元" value="sales3" title="销售额" checked>													<input type="radio" name="comContent3" data-unit="个" value="mids3" title="主要商户数" >													<input type="radio" name="comContent3" data-unit="笔" value="salesNums3" title="交易笔数">												</div>											</div>											<div class="app-statistics app-statistics-left">											<div id="relationshipAnalysis1"></div>											</div>											<div class="app-statistics app-statistics-right">											<div id="relationshipAnalysis2"></div>											</div>										</form>									</div>								</div>							</div>						</div>					</div>				</div>			</div>		</div>		<jsp:include page="appFooter.jsp"></jsp:include>				<script type="text/javascript" src="<%=basePath%>thirdjs/echarts.min.js" charset="utf-8"></script>		<script type="text/javascript" src="<%=basePath%>thirdjs/china.js" charset="utf-8"></script>		<script type="text/javascript" src="<%=basePath%>js/app/show.js" charset="utf-8"></script>	</div></body></html>