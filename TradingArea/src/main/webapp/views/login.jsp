<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %><%    String path = request.getContextPath();    String basePath = request.getScheme() + "://" + request.getServerName() + ":" + request.getServerPort() + path + "/";%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html><head>    <title>商圈平台管理系统</title>    <script type="text/javascript">        var _basePath = '<%=basePath%>';    </script>    <meta http-equiv="pragma" content="no-cache">    <meta http-equiv="cache-control" content="no-cache">    <meta http-equiv="expires" content="0">    <meta http-equiv="keywords" content="keyword1,keyword2,keyword3">    <meta http-equiv="description" content="This is my page">    <jsp:include page="adminCommon.jsp"></jsp:include>    <link rel="stylesheet" href="<%=basePath %>css/login.css">    <link rel="shortcut icon" href="/favicon.ico"/>    <link rel="bookmark" href="/favicon.ico"/></head><body scroll="no" class="login-body"><div scroll="no" class="login"></div><div class="login-left">商圈生态信息平台</div><div class="container layui-layout layui-layout-admin">    <form class="layui-form form" action="">        <div class="layui-form-item login-form-item">            <div class="login-title">                Sign In            </div>        </div>        <div class="layui-form-item login-form-item">            <label class="layui-form-label login-label">登录名</label>            <div class="layui-input-inline">                <input type="text" name="loginName" lay-verify="required" autocomplete="off" placeholder="请输入登录名"                       class="layui-input">            </div>        </div>        <div class="layui-form-item login-form-item">            <label class="layui-form-label login-label">密码</label>            <div class="layui-input-inline">                <input type="password" name="password" lay-verify="required" autocomplete="off" placeholder="请输入密码"                       class="layui-input">            </div>        </div>        <div class="layui-form-item login-form-item">            <label class="layui-form-label">&nbsp;</label>            <div class="layui-input-inline">                <button id="loginFunc" lay-submit lay-filter="formSave"                        class="layui-btn layui-btn-sm layui-btn-warm login-btn"><i class="layui-icon">&#xe609;</i> 登 录                </button>            </div>        </div>    </form></div><script type="text/javascript" src="<%=basePath%>js/app/login.js" charset="utf-8"></script></body></html>