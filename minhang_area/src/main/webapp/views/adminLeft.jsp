<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%String path = request.getContextPath();String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %><div class="layui-side layui-bg-gray">	<div class="layui-side-scroll">		<ul class="layui-nav layui-nav-tree">			<li class="layui-nav-item layui-nav-itemed">				<dl class="layui-nav-child">					<dd id="sysAreaMsg" class="nav-left-menu-dd">						<a class="nav-left-menu-a" href="<%=basePath%>sys/area/list"><i class="layui-icon">&#xe632;</i>&nbsp;&nbsp;区域管理</a>					</dd>					<dd id="sysUserMsg" class="nav-left-menu-dd">						<a class="nav-left-menu-a" href="<%=basePath%>sys/user/list"><i class="layui-icon">&#xe612;</i>&nbsp;&nbsp;用户管理</a>					</dd>					<dd id="sysTradeMsg" class="nav-left-menu-dd">						<a class="nav-left-menu-a" href="<%=basePath%>sys/trade/list"><i class="layui-icon">&#xe600;</i>&nbsp;&nbsp;街道镇管理</a>					</dd>					<dd id="sysTradeDataMsg" class="nav-left-menu-dd">						<a class="nav-left-menu-a" href="<%=basePath%>sys/tradeData/list"><i class="layui-icon">&#xe628;</i>&nbsp;&nbsp;数据维护</a>					</dd>									</dl>			</li>					</ul>	</div></div><script type="text/javascript" src="<%=basePath %>layui/layui.js" charset="utf-8"></script><script>layui.use(['layer','element'], function() {	 layer = layui.layer;	var element = layui.element; //导航的hover效果、二级菜单等功能，需要依赖element模块 	var $ = layui.jquery; 	var url = window.location.href; 	// 区域管理 	if (url.indexOf("/sys/area") >= 0) { 		$("#sysAreaMsg").addClass("layui-this"); 	} 	// 用户管理 	else if (url.indexOf("/sys/user") >= 0) { 		$("#sysUserMsg").addClass("layui-this"); 	} 	// 商圈数据维护 	else if (url.indexOf("/sys/tradeData") >= 0) { 		$("#sysTradeDataMsg").addClass("layui-this"); 	} 	// 商圈管理 	else if (url.indexOf("/sys/trade") >= 0) { 		$("#sysTradeMsg").addClass("layui-this"); 	} 	// 商业体数据维护 	else if (url.indexOf("/sys/businessData") >= 0) { 		$("#sysBusinessDataMsg").addClass("layui-this"); 	} 	// 商业体管理 	else if (url.indexOf("/sys/business") >= 0) { 		$("#sysBusinessMsg").addClass("layui-this"); 	} 	});</script>